<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="CorreosExpress_RegistroDeEnvios::css/bootstrap.min.css"       rel="stylesheet"  type="text/css"/>
        <css src="CorreosExpress_RegistroDeEnvios::css/gijgo.min.css"           rel="stylesheet"  type="text/css"/>
        <css src="CorreosExpress_RegistroDeEnvios::css/introjs.css"             rel="stylesheet"  type="text/css"/>
